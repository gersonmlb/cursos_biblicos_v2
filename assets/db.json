        "_id": "lfdj05",
        "leccion": "05",
        "titulo": "SEÑALES DEL REGRESO DE CRISTO",
        "preguntas": [
            {
                "id": 1,
                "subtitulo": "¿CUÁNDO VENDRÁ?",
                "nomaudio": "",
                "audio": "https://www.w3schools.com/tags/horse.mp3",
                "nomvideo": "",
                "video": "",
                "versiculo": "S. Mateo 24:3",
                "versover": "",
                "prg": "¿QUÉ PREGUNTARON LOS DISCÍPULOS DE JESÚS?",
                "rpt": "Cuales serian las señales de su venida"
            },
            {
                "id": 2,
                "subtitulo": "",
                "nomaudio": "",
                "audio": "https://www.w3schools.com/tags/horse.mp3",
                "nomvideo": "",
                "video": "",
                "versiculo": "S. Mateo 24:36",
                "versover": "",
                "prg": "¿SE SABE LA FECHA DEL REGRESO DE JESÚS ?",
                "rpt": "Ni Dios ni los angeles los saben, solo el padre que esta en los cielos"
            },
            {
                "id": 3,
                "subtitulo": "CÓMO VENDRÁ JESÚS",
                "nomaudio": "",
                "audio": "https://www.w3schools.com/tags/horse.mp3",
                "nomvideo": "",
                "video": "",
                "versiculo": "1 Tes. 5:1-4",
                "versover": "",
                "prg": "¿ESTAMOS EN TINIEBLAS RESPECTO A LA VENIDA DE JESÚS?",
                "rpt": "No, porque si se sabe las señales que habran cuando él venga"
            },
            {
                "id": 4,
                "subtitulo": "SEÑALES QUE ANUNCIAN SU REGRESO",
                "nomaudio": "",
                "audio": "https://www.w3schools.com/tags/horse.mp3",
                "nomvideo": "",
                "video": "",
                "versiculo": "S. Mateo 24:6,7",
                "versover": "",
                "prg": "¿QUÉ SEÑALES PREDIJO JESÚS?",
                "rpt": "Pestes, guerras, señales en la luna y el sol, y en todo el planeta"
            },
            {
                "id": 5,
                "subtitulo": "",
                "nomaudio": "",
                "audio": "https://www.w3schools.com/tags/horse.mp3",
                "nomvideo": "",
                "video": "",
                "versiculo": "Santiago 5:1-5",
                "versover": "",
                "prg": "¿QUÉ CONDICIONES SOCIALES PREVALECERÍAN?",
                "rpt": "Ninguna condicion social "
            },
            {
                "id": 6,
                "subtitulo": "",
                "nomaudio": "",
                "audio": "https://www.w3schools.com/tags/horse.mp3",
                "nomvideo": "",
                "video": "",
                "versiculo": "2 Timoteo 3:1-5",
                "versover": "",
                "prg": "¿CUÁL SERÍA LA CONDICIÓN MORAL DE LA HUMANIDAD?",
                "rpt": "El amor de muchos de acabara"
            },
            {
                "id": 7,
                "subtitulo": "",
                "nomaudio": "",
                "audio": "https://www.w3schools.com/tags/horse.mp3",
                "nomvideo": "",
                "video": "",
                "versiculo": "Daniel 12:4",
                "versover": "",
                "prg": "¿QUÉ OCURRIRÍA CON LA CIENCIA?",
                "rpt": "La ciencia aumentara"
            },
            {
                "id": 8,
