                "nomaudio": "",
                "audio": "https://www.w3schools.com/tags/horse.mp3",
                "nomvideo": "",
                "video": "",
                "versiculo": "S. Mateo 24:29",
                "versover": "",
                "prg": "¿QUÉ SENALES SE VERIAN EN LOS ASTROS?",
